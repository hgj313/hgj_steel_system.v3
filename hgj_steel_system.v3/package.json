{
  "name": "hgj-steel-system-v3",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm:start-server\" \"npm:start-client\"",
    "start-server": "node server/stable.js",
    "start-client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install && cd ../netlify/functions && npm install",
    "build:netlify": "npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "npm start --prefix client",
    "server": "node server/index.js",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lowdb": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pdfkit": "^0.17.1",
    "three": "^0.177.0",
    "uuid": "^9.0.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "netlify-cli": "^17.23.2"
  }
}
